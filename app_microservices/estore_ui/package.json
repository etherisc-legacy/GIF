{
  "name": "estore_ui",
  "version": "1.0.0",
  "description": "eStore UI",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/etherisc/dip-platform"
  },
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "build": "webpack",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "setupFiles": [
      "<rootDir>/internals/enzyme-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "evergreen-ui": "^4.23.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.19",
    "react-router-dom": "^5.1.2",
    "react-stripe-elements": "^6.0.1",
    "sanitize.css": "^11.0.0",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "svg-url-loader": "^3.0.3",
    "url-loader": "^3.0.0",
    "uuid": "^3.4.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "websocket-as-promised": "^1.0.1"
  }
}
